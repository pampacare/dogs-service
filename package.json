{
  "name": "dogs-service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "compilerOptions": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  },
  "scripts": {
    "dev": "dotenv -e .dev.env && yarn model:sync && yarn server:dev",
    "server:dev": "nodemon --files src/server.ts",
    "prod-model:sync": "dotenv -e .dev.env -- yarn model:sync",
    "model:sync": "prisma migrate dev",
    "build": "dotenv -e .env && yarn model:sync && tsc --project",
    "test": "jest --passWithNoTests --runInBand"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^15.0.1",
    "@types/node-fetch": "^2.5.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.3.1",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "git-commit-msg-linter": "^3.1.0",
    "jest": "^27.2.0",
    "nodemon": "^2.0.12",
    "prisma": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@prisma/client": "^3.0.2",
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "uuid": "^8.3.2"
  }
}
